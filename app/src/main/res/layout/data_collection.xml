                                            android:layout_marginLeft="1.0dp"
                                            android:layout_marginRight="1.0dp"
                                            android:layout_gravity="center_horizontal" />

                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="55dp"
                                            android:layout_weight="0.25"
