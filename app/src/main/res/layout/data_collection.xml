                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/ServeButtun">

                        <ScrollView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/scrollView4"
                            android:layout_alignParentEnd="false">

                            <TableLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:id="@+id/ServeData_Sheet">

                                <TableRow
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="#000000"
                                    android:orientation="horizontal">


                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="0.3"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="55dp"
                                            android:layout_weight="0.3"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:text=" 背番号 "
                                            android:id="@+id/ServeUniformNumber"
                                            android:textSize="25dp"
                                            android:background="#FFFFFF"
                                            android:layout_marginTop="0.5dp"
                                            android:layout_marginBottom="0.5dp"
                                            android:layout_marginLeft="1.0dp"
                                            android:layout_marginRight="1.0dp"
                                            android:layout_gravity="center_horizontal" />

                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="55dp"
                                            android:layout_weight="0.7"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:text="     選手名     "
                                            android:id="@+id/ServePlayerName"
                                            android:textSize="25dp"
                                            android:background="#FFFFFF"
                                            android:layout_marginTop="0.5dp"
                                            android:layout_marginBottom="0.5dp"
                                            android:layout_marginLeft="1.0dp"
                                            android:layout_marginRight="1.0dp"
                                            android:layout_gravity="center_horizontal" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        android:layout_weight="0.7">

                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="55dp"
                                            android:layout_weight="0.30"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:text=" 得点 "
                                            android:id="@+id/ServeTokuten"
                                            android:textSize="25dp"
                                            android:background="#FFFFFF"
                                            android:layout_marginTop="0.5dp"
                                            android:layout_marginBottom="0.5dp"
                                            android:layout_marginLeft="1.0dp"
                                            android:layout_marginRight="1.0dp"
                                            android:layout_gravity="center_horizontal" />

                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="55dp"
                                            android:layout_weight="0.30"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:text=" 効果 "
                                            android:id="@+id/ServeKouka"
                                            android:textSize="25dp"
                                            android:background="#FFFFFF"
                                            android:layout_marginTop="0.5dp"
                                            android:layout_marginBottom="0.5dp"
                                            android:layout_marginLeft="1.0dp"
                                            android:layout_marginRight="1.0dp"
                                            android:layout_gravity="center_horizontal" />

                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="55dp"
                                            android:layout_weight="0.30"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:text=" 失点 "
                                            android:id="@+id/ServeSitten"
                                            android:textSize="25dp"
                                            android:background="#FFFFFF"
                                            android:layout_marginTop="0.5dp"
                                            android:layout_marginBottom="0.5dp"
                                            android:layout_marginLeft="1.0dp"
                                            android:layout_marginRight="1.0dp"
                                            android:layout_gravity="center_horizontal" />

                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="55dp"
                                            android:layout_weight="0.1"
                                            android:textAppearance="?android:attr/textAppearanceMedium"
                                            android:text=" 本数 "
                                            android:id="@+id/ServeHonsuu"
                                            android:textSize="25dp"
                                            android:background="#FFFFFF"
                                            android:layout_marginTop="0.5dp"
                                            android:layout_marginBottom="0.5dp"
                                            android:layout_marginLeft="1.0dp"
                                            android:layout_marginRight="1.0dp"
                                            android:layout_gravity="center_horizontal" />

                                    </LinearLayout>
                                </TableRow>
                            </TableLayout>
                        </ScrollView>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/tab3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/ServeCutButtun"
                        android:weightSum="1">

                        <Button
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="追加"
                            android:id="@+id/ServeCutAddPlayer"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentStart="false" />

                        <EditText
                            android:layout_width="78dp"
                            android:layout_height="wrap_content"
                            android:inputType="number"
                            android:hint="背番号入力"
                            android:ems="10"
                            android:id="@+id/ServeCutNumberInput"
                            android:layout_weight="0.06" />

                        <Button
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="削除"
                            android:id="@+id/ServeCutDeletePlayer"
                            android:layout_alignParentBottom="true"
                            android:layout_toEndOf="@+id/addPlayer" />

                        <EditText
                            android:layout_width="78dp"
                            android:layout_height="wrap_content"
                            android:inputType="number"
                            android:hint="背番号入力"
                            android:ems="10"
                            android:id="@+id/ServeCutDeliteNumberInput"
                            android:layout_weight="0.06" />

                        <Button
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="選手登録"
                            android:id="@+id/ServeCutPlayerTouroku" />

                        <Spinner
                            android:layout_width="wrap_content"
